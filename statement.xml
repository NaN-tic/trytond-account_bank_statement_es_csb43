<tryton>
    <data>
        <record model="ir.ui.view" id="account_bank_statement_form_view">
            <field name="model">account.bank.statement</field>
            <field name="inherit"
                ref="account_bank_statement.account_bank_statement_form_view"/>
            <field name="name">account_bank_statement_form</field>
        </record>
        <record model="ir.model.button" id="button_import_csb43">
            <field name="name">import_csb43</field>
            <field name="model"
                search="[('model', '=', 'account.bank.statement')]"/>
        </record>

        <record model="ir.model.button-res.group"
                id="button_import_csb43_group">
            <field name="button" ref="button_import_csb43"/>
            <field name="group" ref="account.group_account"/>
        </record>

        <record model="ir.ui.view" id="import_csb43_start_view_form">
            <field name="model">account.bank.statement.import_csb43.start</field>
            <field name="type">form</field>
            <field name="name">import_csb43_start_view_form</field>
        </record>

        <record model="ir.action.wizard" id="act_import_csb43">
            <field name="name">Import CSB43</field>
            <field name="wiz_name">account.bank.statement.import_csb43</field>
            <field name="model">account.bank.statement</field>
        </record>
    </data>
</tryton>
