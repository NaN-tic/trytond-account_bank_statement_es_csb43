<tryton>
    <data>
        <record model="ir.model.button" id="csb43_import_file_button">
            <field name="name">Import Statement File</field>
            <field name="model" search="[('model', '=', 'account.bank.statement')]"/>
        </record>

        <record model="ir.model.button-res.group" id="csb43_import_file_group">
            <field name="button" ref="csb43_import_file_button"/>
            <field name="group" ref="account.group_account"/>
        </record>

        <record model="ir.ui.view" id="import_csb43_start_view_form">
            <field name="model">account.bank.statement.import_csb43.start</field>
            <field name="type">form</field>
            <field name="name">import_csb43_start_view_form</field>
        </record>

        <record model="ir.action.wizard" id="act_import_csb43">
            <field name="name">Import CSB43</field>
            <field name="wiz_name">account.bank.statement.import_csb43</field>
            <field name="model">account.bank.statement</field>
        </record>
        <record model="ir.action.keyword" id="import_keyword">
            <field name="keyword">form_action</field>
            <field name="model">account.bank.statement,-1</field>
            <field name="action" ref="act_import_csb43"/>
        </record>
    </data>
</tryton>
